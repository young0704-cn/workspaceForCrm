<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.home.crm.workbench.dao.TranHistoryDao">
	<insert id="save" parameterType="tranHistory">
        insert into tbl_tran_history value (
            #{id},
            #{stage},
            #{money},
            #{expectedDate},
            #{createTime},
            #{createBy},
            #{tranId}
        )
    </insert>
    <select id="getByTranId" resultType="tranHistory">
        select * from tbl_tran_history where tranId=#{id}
    </select>
</mapper>

































